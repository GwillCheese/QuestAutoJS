{
  "builds": [
    {
      "src": "client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/client/pages/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/client/pages/index.html"
    },
    {
      "src": "/cars.html",
      "dest": "/client/pages/cars.html"
    },
    {
      "src": "/about.html", 
      "dest": "/client/pages/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}